# 1.1
def is_multiple(n, m):
    return n // m != 0


# 1.2
# def is_even(n):

# 1.3
def minmax(data):
    min, max = data[0], 0
    for val in data:
        if val > max:
            max = val
        elif val < min:
            min = val
    return min, max

# 1.4 - 1.5
def sum_squares(n):
    return sum([k * k for k in range(1, n + 1) if n >= 1])

# 1.6 - 1.7
def sum_odd_squares(n):
    return sum([k * k for k in range(1, n + 1) if k % 2 != 0])

